{
    "name": "backend-lab",
    "version": "1.0.0",
    "description": "Lab backend",
    "main": "server.js",
    "scripts": {
        "start": "node -r dotenv/config ./src/server",
        "dev": "npx nodemon --trace-warnings -r dotenv/config ./src/server",
        "createdb": "npx sequelize-cli db:create",
        "migratedb": "npx sequelize-cli db:migrate",
        "seeddb": "npx sequelize-cli db:seed:all",
        "build": "npm i && npm run migratedb && npm run seeddb"
    },
    "keywords": [],
    "author": "Rodney Neville",
    "license": "MIT",
    "dependencies": {
        "@sendgrid/mail": "^8.1.1",
        "aes-js": "^3.1.2",
        "axios": "^1.6.7",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "cypress": "^13.13.3",
        "dotenv": "^16.4.4",
        "express": "^4.18.2",
        "express-async-errors": "^3.1.1",
        "helmet": "^7.1.0",
        "jsonwebtoken": "^9.0.2",
        "mongodb": "^6.8.0",
        "mongoose": "^8.5.4",
        "morgan": "^1.10.0",
        "node-schedule": "^2.1.1",
        "sequelize": "^6.37.0",
        "technicalindicators": "^3.1.0",
        "telegraf": "^4.15.3",
        "winston": "^3.11.0",
        "ws": "^8.18.0",
        "@faker-js/faker": "^9.0.1",
        "faker": "^6.6.6"
    }
}
